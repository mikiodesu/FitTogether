<% if workout.liked_by?(current_user) %>
  <% like = workout.likes.find_by(user_id: current_user.id) %>
  <%= link_to "👍 #{workout.likes.count}", workout_like_path(workout, like), method: :delete %>
<% else %>
  <%= link_to "👍 #{workout.likes.count}", workout_likes_path(workout), method: :post %>
<% end %>